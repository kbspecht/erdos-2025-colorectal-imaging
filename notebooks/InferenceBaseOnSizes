ages')\n","\n","def yolo2coco(dir, bbox_yolo):\n","  num_bbox = bbox_yolo.shape[0]\n","  bbox_coco = np.zeros((num_bbox, 4))\n","  img =  Image.open(dir)\n","  for i in range(num_bbox):\n","    x_top_left = img.size[0] * (bbox_yolo['x_center'][i] - 0.5 * bbox_yolo['width'][i])\n","    y_top_left = img.size[1] * (bbox_yolo['y_center'][i] - 0.5 * bbox_yolo['height'][i])\n","\n","    x_bot_right = img.size[0] * (bbox_yolo['x_center'][i] + 0.5 * bbox_yolo['width'][i])\n","    y_bot_right = img.size[1] * (bbox_yolo['y_center'][i] + 0.5 * bbox_yolo['height'][i])\n","    bbox_coco[i,:]= np.array([x_top_left, y_top_left, x_bot_right, y_bot_right])\n","\n","  return bbox_coco\n","\n","def read_gtr_size(image_id):\n","  label_path = label_dir / (image_id + '.txt')\n","  image_path = image_dir / (image_id + '.jpg')\n","\n","  relative_size = 0.0\n","  bbox_yolo = torch.empty((0, 4))\n","  bbox_coco = torch.empty((0, 4))\n","  if label_path.stat().st_size > 0:\n","    bbox_yolo = pd.read_table(label_path, sep = ' ', header = None)\n","    bbox_yolo.columns = ['class_index','x_center', 'y_center', 'width', 'height']\n","    num_bbox = bbox_yolo.shape[0]\n","    bbox_coco = yolo2coco(image_path, bbox_yolo)\n","    for i in range(num_bbox):\n","      relative_size += bbox_yolo.loc[i, 'width'] * bbox_yolo.loc[i, 'height']\n","    bbox_yolo = bbox_yolo.to_numpy()[:,1:]\n","    bbox_yolo = torch.from_numpy(bbox_yolo)\n","    bbox_coco = torch.from_numpy(bbox_coco)\n","  return relative_size, bbox_yolo, bbox_coco"],"metadata":{"id":"sZGUzFJdwH8c"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["info_matrix = [\n","    [\n","        p['file_name'],\n","        p['score'],\n","        torch.tensor([p['bbox']]),\n","        *read_gtr_size(p['image_id'])\n","    ]\n","    for p in preds]\n","\n","info_matrix = pd.DataFrame(info_matrix, columns=[\n","    'file_name',\n","    'score',\n","    'bbox_coco_pred',\n","    'relative_size',\n","    'bbox_yolo_gtr',\n","    'bbox_coco_gtr'])"],"metadata":{"id":"wDyq70hAxpHV"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["info_matrix.to_csv('/content/drive/MyDrive/Colab Notebooks/yolo11s_aug3pt.csv')"],"metadata":{"id":"e5Eby3bHUenH"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["info_matrix['IOU'] = info_matrix.apply(lambda row:box_iou(row['bbox_coco_pred'], row['bbox_coco_gtr']), axis = 1)"],"metadata":{"id":"44WBG-UkbcQy"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["info_at_least1 = info_matrix.loc[info_matrix['relative_size'].apply(lambda x: x > 0.0),:].copy()\n","info_at_least1['IOU'] = info_at_least1['IOU'].apply(lambda x: abs(float(x.max(1)[0])))"],"metadata":{"id":"3L1BMfwxepw2"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["size_iou = info_at_least1[['relative_size', 'IOU','score']].to_numpy().copy()\n","size_iou = size_iou[size_iou[:, 0].argsort()]"],"metadata":{"id":"vg6d6AXCgzQ5"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["grids = np.linspace(0.005, 1, 20)\n","polyp_num = np.ones_like(grids[1:])\n","avg_iou = np.ones_like(grids[1:])\n","avg_score = np.ones_like(grids[1:])\n","\n","for i in range(len(grids)-1):\n"," mask = (size_iou[:,0] > grids[i]) & (size_iou[:,0] < grids[i+1])\n"," polyp_num[i] = mask.sum()\n"," avg_iou[i] = size_iou[mask,1].mean()\n"," avg_score[i] = size_iou[mask,2].mean()"],"metadata":{"id":"hNWt1FohlBQZ"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["polyp_num = polyp_num / polyp_num.sum()\n","fig,ax = plt.subplots(figsize=(5, 5))\n","ax.step(grids, np.concat((np.array([0]),avg_iou)), label = 'average iou')\n","ax.step(grids, np.concat((np.array([0]),avg_score)), label = 'confidence score')\n","ax.step(grids, np.concat((np.array([0]),polyp_num)), label = 'density of images')\n","ax.set_xlabel('polyp size (relative to image size)')\n","ax.set_ylabel('iou/score/proportion')\n","ax.set_title('yolo11s_aug3pt')\n","# ax.set_y